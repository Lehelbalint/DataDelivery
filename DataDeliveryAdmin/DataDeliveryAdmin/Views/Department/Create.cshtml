@model DataDeliveryAdmin.Models.AddDepartmentViewModel
@{
    ViewData["Title"] = "Create";
}
<h1>Add Department</h1>
<form method="post" enctype="multipart/form-data" action="Create" class="mt-5">
    <div class="mb-3">
        <label  class="form-label">Department Name:</label>
        <input type="text"  required asp-for="data.department_name" class="form-control">
    <div class="mb-3">
        <label  class="form-label">Year:</label>
        <input type="number"  asp-for="data.year" class="form-control">
    </div>
    <button type="submit" class="btn btn-primary">Submit</button>
</form>